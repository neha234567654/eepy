    life2.setPosition(15, 165);
    add(life2);
    var life3 = new Text("what bone it is", "15pt ChalkDuster");
    life3.setPosition(15, 190);
    add(life3);
    var life4 = new Text("3) Click answer to answer", "15pt ChalkDuster");
    life4.setPosition(15, 215)
    add(life4);
    var life5 = new Text("4) Start testing you loser", "15pt ChalkDuster");
    life5.setPosition(15, 430);
    add(life5);
    
    var sail = new WebImage("https://d.newsweek.com/en/full/1920025/cat-its-mouth-open.jpg?w=1600&h=1600&q=88&f=b7a44663e082b8041129616b6b73328d");
    sail.setSize(190,150);
    sail.setPosition(getWidth()/2-sail.getWidth()/2, 255);
    add(sail);
    
    backbutton();
    mouseClickMethod(backtesthow);
}

function backtesthow(e){
    var elem = getElementAt(e.getX(), e.getY());
    
    for(var i = 0; i < 17; i++){
        if ((elem != null) && (elem == back) || (elem == backtxt)){
            removeAll();
            backcolor();
            testPage();
        }
    }
}

