function start(){
    startScreen();
}

function backcolor()
{
    var colback = new Rectangle(getWidth(),getHeight());
    colback.setColor("#EAFAFF");
    add(colback);
}

function backbutton()
{
    back.setPosition(getWidth()-60, getHeight()-35);
    back.setColor(Color.BLACK);
    add(back);
    backtxt.setPosition(getWidth()-55, getHeight()-20);
    backtxt.setColor(Color.WHITE);
    add(backtxt);
}

function nextButton()
{
    next.setPosition(getWidth()-120, getHeight()-35);
    next.setColor(Color.BLACK);
    add(next);
    nextxt.setPosition(getWidth()-110, getHeight()-20);
    nextxt.setColor(Color.WHITE);
    add(nextxt);
}

var back = new Rectangle(50,20);
var backtxt = new Text("LEABe", "8pt ChalkDuster");
var next = new Rectangle(50,20);
var nextxt = new Text("Next", "8pt ChalkDuster");
var what = new Rectangle(210,70);
var what2 = new Rectangle(200,60);
var whatxt = new Text("What?", "20pt ChalkDuster");
var learn = new Rectangle(210,70);
var learn2 = new Rectangle(200,60);
var learntxt = new Text("Learn", "20pt Chalkduster");
var test = new Rectangle(210,70);
var test2 = new Rectangle(200,60);
var testxt = new Text("Test", "20pt Chalkduster");

function startScreen(){
    backcolor();
    
    var title = new Text("BONES", "30pt Chalkduster");
    title.setPosition(getWidth()/2-title.getWidth()/2, getHeight()/5);
    add(title);
    
    what.setPosition(getWidth()/2-what.getWidth()/2, 150);
    add(what);
    what2.setPosition(getWidth()/2-what2.getWidth()/2,155);
    what2.setColor("#EAFAFF");
    add(what2);
    whatxt.setPosition(getWidth()/2-whatxt.getWidth()/2,185+10);
    add(whatxt);
    
    
    learn.setPosition(getWidth()/2-what.getWidth()/2, 265);
    add(learn);
    learn2.setPosition(getWidth()/2-what2.getWidth()/2,270);
    learn2.setColor("#EAFAFF");
    add(learn2);
    learntxt.setPosition(getWidth()/2-learntxt.getWidth()/2,310);
    add(learntxt);
    
    
    test.setPosition(getWidth()/2-what.getWidth()/2, 360);
    add(test);
    test2.setPosition(getWidth()/2-what2.getWidth()/2,365);
    test2.setColor("#EAFAFF");
    add(test2);
    testxt.setPosition(getWidth()/2-testxt.getWidth()/2,405);
    add(testxt);
    
    mouseClickMethod(startGame);
}

function startGame(e) {
    var elem = getElementAt(e.getX(), e.getY());

    if ((elem != null) && ((elem == what2)) || (elem == whatxt)) {
        removeAll();
        whatPage();
    }
    if((elem != null) && ((elem == learn2)) || (elem == learntxt)){
        removeAll();
        learnPage();
    }
    if((elem != null) && ((elem == test2)) || (elem == testxt)){
        removeAll();
        testPage();
    }
}

function backtoStart(e){
    var elem = getElementAt(e.getX(), e.getY());
    
    if ((elem != null) && ((elem == back)) || (elem == backtxt)) {
        removeAll();
        startScreen();
        mouseClickMethod(startGame);
    }
}

function whatPage(){
    backcolor();
    
    var title4 = new Text("What?", "30pt Chalkduster");
    title4.setPosition(getWidth()/2-title4.getWidth()/2, getHeight()/7);
    add(title4);
    
    var life = new Text("When i was weawy weawy wittle ", "15pt ChalkDuster");
    life.setPosition(15,130);
    add(life);
    var life2 = new Text("i woke up and this morning i", "15pt ChalkDuster");
    life2.setPosition(15, 155);
    add(life2);
    var life3 = new Text("woke up today in this morning", "15pt ChalkDuster");
    life3.setPosition(15, 180);
    add(life3);
    var life4 = new Text("in the morning i woke up this", "15pt ChalkDuster")
    life4.setPosition(15, 205);
    add(life4);
    var life5 = new Text("morning, i woke up and", "15pt ChalkDuster");
    life5.setPosition(15, 230);
    add(life5);
    var life6 = new Text("remebemer that i wake up, ", "15pt ChalkDuster");
    life6.setPosition(15, 255);
    add(life6);
    var life7 = new Text("i woke, i woke up that. :(", "15pt ChalkDuster");
    life7.setPosition(15, 280);
    add(life7);
    
    backbutton();
    mouseClickMethod(backtoStart);
}

var howTo = new Rectangle(210,60);
var howTo2 = new Rectangle(200,50);
var howTotxt = new Text("How to", "17pt ChalkDuster");
var arms = new Rectangle(210,60);
var arms2 = new Rectangle(200,50);
var armstxt = new Text("Arms", "17pt Chalkduster");
var legs = new Rectangle(210,60);
var legs2 = new Rectangle(200,50);
var legstxt = new Text("Legs", "17pt Chalkduster");
var torso = new Rectangle(210,60);
var torso2 = new Rectangle(200,50);
var torsotxt = new Text("Torso", "17pt Chalkduster");
var head = new Rectangle(210,60);
var head2 = new Rectangle(200,50);
var headtxt = new Text("Head", "17pt Chalkduster");
var answerBox = new Rectangle(200,60);
var answerBoxTxt = new Text("Answer", "25pt Chalkboard");

function buttonPage(){
    howTo.setPosition(getWidth()/2-what.getWidth()/2, 100);
    add(howTo);
    howTo2.setPosition(getWidth()/2-what2.getWidth()/2,105);
    howTo2.setColor("#EAFAFF");
    add(howTo2);
    howTotxt.setPosition(getWidth()/2-whatxt.getWidth()/2,140);
    add(howTotxt);
    
    arms.setPosition(getWidth()/2-what.getWidth()/2, 185);
    add(arms);
    arms2.setPosition(getWidth()/2-what2.getWidth()/2,190);
    arms2.setColor("#EAFAFF");
    add(arms2);
    armstxt.setPosition(getWidth()/2-armstxt.getWidth()/2,225);
    add(armstxt);
    
    legs.setPosition(getWidth()/2-legs.getWidth()/2, 250);
    add(legs);
    legs2.setPosition(getWidth()/2-legs2.getWidth()/2,255);
    legs2.setColor("#EAFAFF");
    add(legs2);
    legstxt.setPosition(getWidth()/2-legstxt.getWidth()/2,290);
    add(legstxt);
    
    torso.setPosition(getWidth()/2-torso.getWidth()/2, 315);
    add(torso);
    torso2.setPosition(getWidth()/2-torso2.getWidth()/2,320);
    torso2.setColor("#EAFAFF");
    add(torso2);
    torsotxt.setPosition(getWidth()/2-torsotxt.getWidth()/2,355);
    add(torsotxt);
    
    head.setPosition(getWidth()/2-head.getWidth()/2, 380);
    add(head);
    head2.setPosition(getWidth()/2-head2.getWidth()/2,385);
    head2.setColor("#EAFAFF");
    add(head2);
    headtxt.setPosition(getWidth()/2-headtxt.getWidth()/2,420);
    add(headtxt);
}

function learnPage(){
    backcolor();
    
    var title2 = new Text("Learn", "30pt Chalkduster");
    title2.setPosition(getWidth()/2-title2.getWidth()/2, getHeight()/7);
    add(title2);
    
    buttonPage();
    mouseClickMethod(startLearn);
    backbutton();
    
}

function testPage(){
    backcolor();
    
    var title3 = new Text("Test", "30pt Chalkduster");
    title3.setPosition(getWidth()/2-title3.getWidth()/2, getHeight()/7);
    add(title3);
    
    backbutton();
    buttonPage();
    mouseClickMethod(startTest);
}

function startLearn(e) {
    var elem3 = getElementAt(e.getX(), e.getY());

    if ((elem3 != null) && ((elem3 == howTo2)) || (elem3 == howTotxt)) {
        removeAll();
        backcolor();
        learnHow();
    }
    if((elem3 != null) && ((elem3 == arms2)) || (elem3 == armstxt)){
        removeAll();
        backcolor();
        learnArms();
    }
    if((elem3 != null) && ((elem3 == legs2)) || (elem3 == legstxt)){
        removeAll();
        backcolor();
        learnLegs();
    }
    if((elem3 != null) && ((elem3 == torso2)) || (elem3 == torsotxt)){
        removeAll();
        backcolor();
        learnTorso();
    }
    if((elem3 != null) && ((elem3 == head2)) || (elem3 == headtxt)){
        removeAll();
        backcolor();
        learnHead();
    }
    if ((elem3 != null) && ((elem3 == back)) || (elem3 == backtxt)) {
        removeAll();
        backcolor();
        startScreen();
    }
}

function learnHow()
{
    var learnTitle = new Text("Instructions", "30pt ChalkDuster")
    learnTitle.setPosition(getWidth()/2-learnTitle.getWidth()/2, getHeight()/5);
    add(learnTitle);
    var life = new Text("1) Click on the section of body ", "15pt ChalkDuster");
    life.setPosition(15,140);
    add(life);
    var life2 = new Text("2) Click on the bone you want", "15pt ChalkDuster");
    life2.setPosition(15, 165);
    add(life2);
    var life3 = new Text("3) Start learning you loser", "15pt ChalkDuster");
    life3.setPosition(15, 390);
    add(life3);
    var sai2l = new WebImage("https://images.ctfassets.net/usf1vwtuqyxm/DxKfqdm0vlM656PFLOrcv/17bc8ff3180ce8c430354fb143043cee/Dobby_2_1800x1248.png");
    sai2l.setSize(190,150);
    sai2l.setPosition(getWidth()/2-sai2l.getWidth()/2, 200);
    add(sai2l);
    
    backbutton();
    mouseClickMethod(backlearnhow);

}

function backlearnhow(e){
    var elem = getElementAt(e.getX(), e.getY());
    
    for(var i = 0; i < 17; i++){
        if ((elem != null) && (elem == back) || (elem == backtxt)){
            removeAll();
            backcolor();
            learnPage();
        }
    }
}

var clavicles = new Text("Clavicles", "15pt ChalkDuster");
var scapula = new Text("Scapula", "15pt ChalkDuster");
var humerus = new Text("Humerus", "15pt ChalkDuster");
var radius = new Text("Radius", "15pt ChalkDuster");
var ulna = new Text("Ulna", "15pt ChalkDuster");
var scaphoid = new Text("Scaphoid", "15pt ChalkDuster");
var lunate = new Text("Lunate", "15pt ChalkDuster");
var triquetrum = new Text("Triquetrum", "15pt ChalkDuster");
var pisiform = new Text("Pisiform", "15pt ChalkDuster");
var trapezium = new Text("Trapezium", "15pt ChalkDuster");
var trapezoid = new Text("Trapezoid", "15pt ChalkDuster");
var capitate = new Text("Capitate", "15pt ChalkDuster");
var hamate = new Text("Hamate", "15pt ChalkDuster");
var metacarpal = new Text("Metacarpal", "15pt ChalkDuster");
var proximalphalanges = new Text("Proximal Phalanges", "15pt ChalkDuster");
var intermediatephalanges = new Text("Intermediate Phalanges", "15pt ChalkDuster");
var distalphalanges = new Text("Distal Phalanges", "15pt ChalkDuster");
var armBoneArr2 = [clavicles, scapula, humerus, radius, ulna, scaphoid, lunate, triquetrum, pisiform, trapezium, trapezoid, capitate, hamate, metacarpal, proximalphalanges, intermediatephalanges, distalphalanges];

function learnArms()
{
    for(var i = 0; i < 17; i++)
    {
        armBoneArr2[i].setPosition(20, 30+27*i);
    }
    for(var i = 0; i < 17; i++)
    {
        add(armBoneArr2[i]);
    }
    
    backbutton();
    mouseClickMethod(clickarmlearn);
}

function clickarmlearn(e){
    var elem = getElementAt(e.getX(), e.getY());
    
    for(var i = 0; i < 17; i++){
        if ((elem != null) && (elem == armBoneArr2[i])){
            removeAll();
            backcolor();
            displayit(i, armBoneArr, armBonesDef, armBones, learnArms);
        }
    }
    
    if ((elem != null) && ((elem == back)) || (elem == backtxt)) {
        removeAll();
        backcolor();
        learnPage();
    }
}



var armBonesDef = new Map([
        ["clavicles", "two slender, S-shapes bones that connect*the sternum to the scapulae' they support the upper*limbs, allow for a wide range of arm movements,*and protect the neurovascular structures of the neck"],
        ["scapula", "a bone on the back of the thorax that*provides attachment points for muscles, allows for*a wide range of shoulder movements, and protects the*neurovascular structures of the shoulder."],
        ["humerus", "the long bone*in the upper arm that*allows for movement*and protects the neurovascular structures"],
        ["radius", "one of the two bones in the forearm*that supports the forearm and hand,*allows for rotation of the forearm,*and provides attachment points for muscles"],
        ["ulna", "one of the two bones in the forearm*that supports the forearm and hand,*provides stability for the elbow joint,*and provides attachment points for muscles"],
        ["scaphoid", "a bone in the wrist*that supports the wrist,*allows for movement of the wrist,*and provides attachment points for muscles"],
        ["lunate", "a bone in the center of the wrist*that supports the wrist, allows*for movement of the wrist,*and provides attachment points for muscles"],
        ["triquetrum", "a bone in the wrist that supports*the wrist, allows for movement*of the wrist, and provides attachment*points for muscles and ligaments"],
        ["pisiform", "a small bone in the wrist that protects*the tendon of the flexor carpi ulnaris muscle,*provides stability for the wrist, and provides*an attachment point for the abductor digiti minimi muscle"],
        ["trapezium", "a bone in the wrist that*supports the wrist, allows for movement*of the wrist, and provides attachment*points for muscles that move the thumb."],
        ["trapezoid", "a bone in the wrist that supports*the wrist, allows for movement of the*wrist, and provides attachment points*for muscles that move the wrist and hand"],
        ["capitate", "a carpal bone in the wrist that*supports the wrist, allows for movement of the*wrist, and provides attachment points*for muscles that move the wrist, hand, and thumb"],
        ["hamate", "a bone in the wrist that supports the*wrist, allows for movement of the wrist,*and provides attachment points for muscles that*move the wrist, little finger, and ring finger"],
        ["metacarpal", "the five long bones in the hand that connect the*wrist bones to the finger bones. They transmit forces*from the hand to the wrist and forearm, support*the hand and fingers, and allow for movement of the fingers"],
        ["proximal phalanges", "the first bones in the fingers*and toes that transmit*forces, support the fingers and*toes, and allow for movement."],
        ["intermediate phalanges", "the middle bones of the fingers*and toes that transmit*forces, support the fingers and*toes, and allow for movement"],
        ["distal phalanges", "the bones at the tips of the fingers*and toes that protect the fingertips*and toenails, allow for fine motor*movements, and provide sensation"],
          
    ]);
    
var femur = new Text("Femur", "15pt ChalkDuster");
var patella = new Text("Patella", "15pt ChalkDuster");
var tibia = new Text("Tibia", "15pt ChalkDuster");
var fibula = new Text("Fibula", "15pt ChalkDuster");
var calcaneus = new Text("Calcaneus", "15pt ChalkDuster");
var talus = new Text("Talus", "15pt ChalkDuster");
var navicular = new Text("Navicular", "15pt ChalkDuster");
var cuneiform = new Text("Cuneiform", "15pt ChalkDuster");
var cuboid = new Text("Cuboid", "15pt ChalkDuster");
var metatarsal = new Text("Metatarsal", "15pt ChalkDuster");
var proximalphalanges = new Text("Proximal Phalanges", "15pt ChalkDuster");
var intermediatephalanges = new Text("Intermediate Phalanges", "15pt ChalkDuster");
var distalphalanges = new Text("Distal Phalanges", "15pt ChalkDuster");
var legBonesArr2 = [femur, patella, tibia, fibula, calcaneus, talus, navicular, cuneiform, cuboid, metatarsal, proximalphalanges, intermediatephalanges, distalphalanges];
function learnLegs()
{
    for(var i = 0; i < 13; i++)
    {
        legBonesArr2[i].setPosition(20, 30+27*i);
    }
    for(var i = 0; i < 13; i++)
    {
        add(legBonesArr2[i]);
    }
    
    backbutton();
    mouseClickMethod(clickleglearn);
}

function clickleglearn(e){
    var elem = getElementAt(e.getX(), e.getY());
    
    for(var i = 0; i < 13; i++){
        if ((elem != null) && (elem == legBonesArr2[i])){
            removeAll();
            backcolor();
            displayit(i, legBonesArr, legBonesDef, legBones, learnLegs);
        }
    }
    
    if ((elem != null) && ((elem == back)) || (elem == backtxt)) {
        removeAll();
        backcolor();
        learnPage();
    }
}
    
var legBonesDef = new Map([
        ["femur", "the longest and strongest bone in the*human body that supports the weight of*the body, allows for movement of the leg,*and protects the nerves and blood vessels of the leg."],
        ["patella", "the kneecap, a bone that protects*the knee joint, increases the leverage*of the quadriceps femoris muscle,*and reduces friction in the knee joint."],
        ["tibia", "the shinbone, a large, strong bone*that supports the weight of the body,*allows for movement of the leg, and protects*the nerves and blood vessels of the leg"],
        ["fibula", "the calf bone, a smaller, lateral, and*non-weight-bearing bone that provides stability*to the ankle joint, allows for muscle attachment,*and protects the nerves and blood vessels of the leg"],
        ["calcaneus", "the heel bone, a large bone*that supports the weight of the body,*allows for movement of the foot, and protects*the nerves and blood vessels of the foot"],
        ["talus", "the anklebone, a small, irregularly shaped*bone that transmits forces from the leg to*the foot, allows for movement of the ankle joint,*and provides stability to the ankle joint."],
        ["navicular", "a bone in the midfoot that transmits forces*from the talus bone to the cuneiform bones,*allows for movement of the midfoot joint,*and provides stability to the midfoot joint"],
        ["cuneiform", "three small, wedge-shaped bones in the*midfoot that transmit forces from the midfoot to*the forefoot, provide stability to the midfoot joint,*and allow for movement of the midfoot joints"],
        ["cuboid", "a bone in the midfoot that transmits forces*from the midfoot to the forefoot, provides*stability to the lateral column of the foot,*and allows for movement of the lateral column of the foot"],
        ["metatarsal", "five long bones in the midfoot*that support the weight of the body,*allow for movement of the foot,*and provide attachment points for muscles"],
        ["proximal phalanges", "the first bones in the fingers*and toes that transmit forces,*support the fingers and toes,*and allow for movement"],
        ["intermediate phalanges", "the middle bones of the fingers*and toes that transmit forces,*support the fingers and toes,*and allow for movement"],
        ["distal phalanges", "the bones at the tips of the fingers*and toes that protect the fingertips*and toenails, allow for fine motor*movements, and provide sensation"],
    ]);
    
