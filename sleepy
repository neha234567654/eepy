var headBonesDef = new Map([
        ["frontal", "the bone that forms the forehead and*roofs over most of the orbits and nasal cavity.*It protects the brain, supports the skull, provides attachment*points for muscles, and houses the frontal sinuses"],
        ["parietal", "the bone that forms the sides*and top of the skull. It protects*the brain, supports the skull, and*provides attachment points for muscles"],
        ["temporal", "the bone that protects the brain, supports*the skull, houses the organs of hearing and*balance, supports the temporomandibular joint, and*provides passageways for blood vessels and nerves"],
        ["occipital", "the bone that protects the brain, supports*the skull, provides attachment points for*muscles and ligaments, and houses the foramen magnum*"],
        ["sphenoid", "the bone that supports the skull,*protects the brain and nerves, houses*the pituitary gland and sella turcica, and*contributes to the formation of the sinuses."],
        ["nasal", "the bone that forms the bridge of*the nose, protects the nasal cavity,*supports the nose, and allows for airflow.*"],
        ["maxilla", "the upper jawbone that supports the upper teeth,*provides attachment points for facial muscles, forms the*hard palate, contributes to the formation of the lateral*walls of the nasal cavity, and houses the maxillary sinus."],
        ["zygomatic", "the cheekbone that supports the soft tissues of*the cheek, provides attachment points for facial*muscles, forms the zygomatic arch, and contributes*to the formation of the lateral wall of the orbit"],
        ["palatine", "the posterior part of the hard palate,*contributes to the formation of the*lateral wall of the nasal cavity, provides attachment*points for facial muscles, and houses the palatine canals"],
        ["lacrimal", "the bone in the eye socket that protects*the lacrimal sac and nasolacrimal duct, supports*the medial wall of the orbit, forms the lacrimal fossa, and*contributes to the formation of the nasolacrimal canal"],
        ["inferior nasal conchae", "extends horizontally along the*lateral wall of the nasal cavity*and consists of a lamina of spongy bone,*curled upon itself like a scroll,"],
        ["vomer", "a thin bone that forms the inferior part of*the nasal septum and provides support,*attachment for mucous membranes, airflow direction,*and passage for blood vessels and nerves"],
        ["mandible", "the lower jawbone that supports the lower*teeth, provides attachment points for*facial muscles, protects the soft tissues of*the face, and contributes to the formation of the airway"],
        ["hyoid", "the bone in the neck that supports*the tongue and larynx, protects the *airway, facilitates swallowing, and provides*attachment points for muscles and ligaments"],
        ["malleus", "the outermost ossicle in the middle*ear that transmits sound vibrations from*the eardrum to the inner ear, protects the*inner ear, and maintains the stability of the eardrum"],
        ["incus", "a bone in the middle ear.*The anvil-shaped small bone is one of*three ossicles in the middle ear.*The incus receives vibrations from the malleus"],
    ])
    
function displayit(num, arr, map, map2, func)
{
    var title = arr[num];
    var titletxt = new Text(title, "20pt ChalkDuster");
    titletxt.setPosition(getWidth()/2-titletxt.getWidth()/2, getHeight()/8);
    add(titletxt);
    
    var im = new WebImage(map2.get(arr[num]));
    im.setSize(160, 230);
    im.setPosition(getWidth()/2-im.getWidth()/2, getHeight()/8+30);
    add(im);
    
    var str = map.get(arr[num]);
    var temp1 = str.substring(0, str.indexOf("*"));
    str = str.substring(str.indexOf("*")+1);
    var temp2 = str.substring(0, str.indexOf("*"));
    str = str.substring(str.indexOf("*")+1);
    var temp3 = str.substring(0, str.indexOf("*"));
    str = str.substring(str.indexOf("*")+1);
    var temp4 = str;
    var print1 = new Text(temp1, "8pt ChalkDuster");
    print1.setPosition(getWidth()/2-print1.getWidth()/2, 370);
    add(print1);
    var print2 = new Text(temp2, "8pt ChalkDuster");
    print2.setPosition(getWidth()/2-print2.getWidth()/2, 385);
    add(print2);
    var print3 = new Text(temp3, "8pt ChalkDuster");
    print3.setPosition(getWidth()/2-print3.getWidth()/2, 400);
    add(print3);
    var print4 = new Text(temp4, "8pt ChalkDuster");
    print4.setPosition(getWidth()/2-print4.getWidth()/2, 415);
    add(print4);
    
    function backfrombone(e){
        var elem = getElementAt(e.getX(), e.getY());

        if ((elem != null) && ((elem == back)) || (elem == backtxt)) {
            removeAll();
            backcolor();
            func();
        }
    }
    
    backbutton();
    mouseClickMethod(backfrombone);
}

function startTest(e) {
    var elem3 = getElementAt(e.getX(), e.getY());

    if ((elem3 != null) && ((elem3 == howTo2)) || (elem3 == howTotxt)) {
        removeAll();
        backcolor();
        testHow();
    }
    if((elem3 != null) && ((elem3 == arms2)) || (elem3 == armstxt)){
        removeAll();
        backcolor();
        testArms();
    }
    if((elem3 != null) && ((elem3 == legs2)) || (elem3 == legstxt)){
        removeAll();
        backcolor();
        testLegs();
    }
    if((elem3 != null) && ((elem3 == torso2)) || (elem3 == torsotxt)){
        removeAll();
        backcolor();
        testTorso();
    }
    if((elem3 != null) && ((elem3 == head2)) || (elem3 == headtxt)){
        removeAll();
        backcolor();
        testHead();
    }
    if((elem3 != null) && ((elem3 == back)) || (elem3 == backtxt)){
        removeAll();
        backcolor();
        startScreen();
    }
}

