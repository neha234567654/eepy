var num;
var im;
var ans2 = new Rectangle(190,50);
var anstxt = new Text("answer", "15pt ChalkDuster");
var userinp;
function qpage(nume, arr, map){
    num = Randomizer.nextInt(0, nume);
    
    im = new WebImage(map.get(arr[num]));
    im.setSize(160,230);
    im.setPosition(getWidth()/2-im.getWidth()/2, 100)
    add(im);
    
    var ask = new Text("What bone is this?", "20pt ChalkDuster");
    ask.setPosition(getWidth()/2-ask.getWidth()/2, 50);
    add(ask);
    
    var ans = new Rectangle(200, 60);
    ans.setPosition(getWidth()/2-what.getWidth()/2, 370);
    add(ans);
    ans2.setPosition(getWidth()/2-what2.getWidth()/2,375);
    ans2.setColor("#EAFAFF");
    add(ans2);
    anstxt.setPosition(getWidth()/2-anstxt.getWidth()/2,405);
    add(anstxt);
    
    function userans(e){
        var elem = getElementAt(e.getX(), e.getY());

        if ((elem != null) && ((elem == ans2)) || (elem == anstxt)) {
            userinp = readLine("Type in your answer: ");
            check(arr, userinp, num, nume, map);
        }
    }
    
    mouseClickMethod(userans);
}

function check(arr, response, number, nume, map){
    if (arr[number] == response){
        removeAll();
        backcolor();
        youGotRight();
    }
    else{
        removeAll();
        backcolor();
        youGotWrong(arr, number);
    }
    
    function userpick(e){
        var elem = getElementAt(e.getX(), e.getY());
    
        if ((elem != null) && ((elem == back)) || (elem == backtxt)) {
            removeAll();
            testPage();
        }
        if ((elem != null) && ((elem == next)) || (elem == nextxt)) {
            removeAll();
            backcolor();
            qpage(nume, arr, map);
        }
    }
    
    backbutton();
    nextButton();
    mouseClickMethod(userpick);
}

