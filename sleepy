function youGotRight(){
    var wrdYou = new Text("You", "40pt ChalkDuster");
    wrdYou.setPosition(getWidth()/2-wrdYou.getWidth()/2, 100)
    add(wrdYou);
    
    var wrdGotIt = new Text("Got It", "40pt ChalkDuster");
    wrdGotIt.setPosition(getWidth()/2-wrdGotIt.getWidth()/2, 160)
    add(wrdGotIt);
    
    var wrdRight = new Text("Right!!!", "40pt ChalkDuster");
    wrdRight.setPosition(getWidth()/2-wrdRight.getWidth()/2, 220)
    add(wrdRight);
    
    var wrdSmile = new Text(":)", "40pt ChalkDuster");
    wrdSmile.setPosition(getWidth()/2-wrdSmile.getWidth()/2, 300)
    add(wrdSmile);
}

function youGotWrong(arr, number){
    var wrdYou = new Text("You", "40pt ChalkDuster");
    wrdYou.setPosition(getWidth()/2-wrdYou.getWidth()/2, 100)
    add(wrdYou);
    
    var wrdGotIt = new Text("Got It", "40pt ChalkDuster");
    wrdGotIt.setPosition(getWidth()/2-wrdGotIt.getWidth()/2, 160)
    add(wrdGotIt);
    
    var wrdRight = new Text("Wrong!!!", "40pt ChalkDuster");
    wrdRight.setPosition(getWidth()/2-wrdRight.getWidth()/2, 220)
    add(wrdRight);
    
    var wrdSmile = new Text(":(", "40pt ChalkDuster");
    wrdSmile.setPosition(getWidth()/2-wrdSmile.getWidth()/2, 300)
    add(wrdSmile);
    
    var correctans = new Text("The correct answer was " + arr[number] + ".", "10pt ChalkDuster");
    correctans.setPosition(getWidth()/2-correctans.getWidth()/2, 350);
    add(correctans);
}
